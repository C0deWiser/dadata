{
    "name": "codewiser/dadata",
    "description": "Plug DaData into laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Codewiser\\Dadata\\": "src/"
        }
    },
    "authors": [
        {
            "name": "pm",
            "email": "pm@101media.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "laravel/framework": ">=10.0",
        "hflabs/dadata": "^22.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codewiser\\Dadata\\DaDataServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3"
    }
}
